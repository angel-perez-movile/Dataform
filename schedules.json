{
  "schedules": [{
    "name": "IncrementalSchedule",
    "options": {
      "includeDependencies": false,
      "fullRefresh": true
    },
    "cron": "7 */1 * * *",
    "notification": {
      "onSuccess": false,
      "onFailure": false
    },
    "notifications": [{
      "events": ["success"],
      "channels": ["Correios"]
    }, {
      "events": ["failure"],
      "channels": ["Correios"]
    }]
  }],
  "notificationChannels": [{
    "name": "Correios",
    "email": {
      "to": ["angel.perez@wavy.global"]
    }
  }]
}